<Window x:Class="DisplayThinkGeoCloudVectorMaps.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:uc1="clr-namespace:ThinkGeo.UI.Wpf;assembly=ThinkGeo.UI.Wpf"
        mc:Ignorable="d"
        Title="MainWindow" Width="1024" Height="768" >
    <Grid>
        <uc1:MapView Name="mapView" Loaded="mapView_Loaded"></uc1:MapView>
        <ComboBox x:Name="styleCmb" HorizontalAlignment="Right" Margin="0,20,10,0" VerticalAlignment="Top" Width="85" Height="32" Opacity="0.36" FontSize="16" FontWeight="Bold"/>
    </Grid>
</Window>
